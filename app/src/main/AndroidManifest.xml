<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bohra.savvysavingappfinal">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.bohra.savvysavingappfinal.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name="com.example.bohra.savvysavingappfinal.SavingSetup" />
        <activity android:name="com.example.bohra.savvysavingappfinal.FixedCostsSetup" />
        <activity android:name="com.example.bohra.savvysavingappfinal.CreatePinActivity" />
        <activity android:name="com.example.bohra.savvysavingappfinal.SetIncomeActivity" />
        <activity android:name="com.example.bohra.savvysavingappfinal.PinEnterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>